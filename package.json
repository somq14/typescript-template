{
  "name": "typescript-template",
  "packageManager": "yarn@3.2.2",
  "scripts": {
    "postinstall": "husky install",
    "clean": "rm -r dist",
    "format": "prettier --ignore-path .gitignore --ignore-unknown --check \"**/*\"",
    "lint": "eslint --ignore-path .gitignore --max-warnings 0 \"**/*.{js,jsx,cjs,mjs,ts,tsx}\"",
    "build": "tsc"
  },
  "devDependencies": {
    "@somq14/eslint-config": "somq14/eslint-config#v1.0.0",
    "eslint": "8.21.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  }
}
